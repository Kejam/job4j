<hibernate-mapping>
    <class name="job4j.carStrorage.logic.items.Car" table="carStorage">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name" column="name"/>
        <property name="engine" column="engine"/>
        <property name="carBody" column="carBody"/>
        <property name="transmission" column="transmission"/>
        <property name="color" column="color"/>
    </class>
</hibernate-mapping>